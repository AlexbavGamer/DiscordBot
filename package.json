{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ts:watch": "tsc -w",
    "ts:build": "tsc",
    "serve": "node dist/main.js",
    "watch": "concurrently \"npm run-script ts:watch\" \"npm run-script serve\"",
    "build": "npm run-script ts:build",
    "prod:serve": "node dist/main.js",
    "start": "npm run-script build && npm run-script prod:serve",
    "tslint": "tslint -c tsconfig.json -p tsconfig.json",
    "botoff": "heroku ps:scale worker=0 --app maytrixxbot",
    "boton": "heroku ps:scale worker=1 --app maytrixxbot"
  },
  "eslintConfig":
  {
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/eslint-disable-line": "off"
    } 
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.5",
    "@types/kue": "^0.11.13",
    "@types/node": "^13.11.0",
    "@types/pg": "^7.14.3",
    "better-sqlite3": "^6.0.1",
    "child_process": "^1.0.2",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "enmap": "^5.2.4",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "kue": "^0.11.6",
    "moment": "^2.24.0",
    "node": "^13.12.0",
    "nodemon": "^2.0.2",
    "pg": "^8.0.0"
  }
}
