{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "ts:watch": "tsc -w",
    "ts:build": "tsc",
    "serve": "node dist/main.js",
    "watch": "concurrently \"npm run-script ts:watch\" \"npm run-script serve\"",
    "build": "npm run-script ts:build",
    "prod:serve": "node dist/main.js",
    "start": "npm run-script prod:serve",
    "tslint": "tslint -c tsconfig.json -p tsconfig.json",
    "botoff": "heroku ps:scale worker=0 --app maytrixxbot && heroku ps:scale web=0 --app maytrixxbot",
    "boton": "heroku ps:scale worker=1 --app maytrixxbot && heroku ps:scale web=1 --app maytrixxbot"
  },
  "eslintConfig": {
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/eslint-disable-line": "off"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.1.0",
    "@types/express": "^4.17.5",
    "@types/express-session": "^1.17.0",
    "@types/kue": "^0.11.13",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^0.7.3",
    "@types/moment-duration-format": "^2.2.2",
    "@types/node": "^13.11.0",
    "@types/passport": "^1.0.3",
    "@types/passport-discord": "^0.1.3",
    "@types/pg": "^7.14.3",
    "@types/yt-search": "^1.1.0",
    "better-sqlite3": "^5.0.0",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "concurrently": "^5.1.0",
    "connect-mongo": "^3.2.0",
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "enmap": "^5.2.4",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-passport": "^0.1.0",
    "express-session": "^1.17.0",
    "ffmpeg": "0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "hastebin-gen": "^2.0.5",
    "helmet": "^3.22.0",
    "kue": "^0.11.6",
    "level-session-store": "^2.2.0",
    "lodash": "^4.17.15",
    "marked": "^0.8.2",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "node": "^13.12.0",
    "node-opus": "^0.3.3",
    "node-superfetch": "^0.1.10",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.3",
    "pg": "^8.0.0",
    "typescript": "^3.8.3",
    "yt-search": "^1.1.2",
    "ytdl-core": "^2.0.1"
  },
  "devDependencies": {
    "@types/helmet": "0.0.45"
  }
}
